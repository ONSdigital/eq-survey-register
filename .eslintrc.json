{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "jasmine": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "plugins": [
        "jasmine"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "script"
    },
    "rules": {
        "quotes":"off",
        "camelcase":"off",
        "brace-style":["error", "stroustrup"]
        
    }
}